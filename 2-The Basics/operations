//Add
let firstNumAdd = 38;
let secNumAdd = 4;
let sum = firstNumAdd + secNumAdd;
console.log(sum);

//Subtraction
let minuend = 44;
let subtrahend = 2;
let diff = minuend - subtrahend;
console.log(diff);

//Multiplication
let firstMult = 7;
let secMult = 6;
let product = firstMult * secMult;
console.log(product);

//Division and Remainder
let dividend = 16;
let divisor = 4;
let quot = dividend / divisor;
console.log(quot);



